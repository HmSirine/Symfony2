                  <RowConstraints minHeight="10.0" prefHeight="300.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
            <Button fx:id="Retour" mnemonicParsing="false" onAction="#RetourMenu" prefHeight="48.0" prefWidth="127.0" style="-fx-background-radius: 30;" text="Retour">
               <textFill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="#545344" />
                        <Stop color="#545344" offset="0.15579710144927536" />
                        <Stop color="#000000f5" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </textFill>
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
